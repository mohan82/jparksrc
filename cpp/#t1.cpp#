#include <stdio.h>

char str[][10] = { "abc", "defg", "h", "ijklmn"};

#define STEP(x) \
	if ((x) == true) printf("This is false\n");



int main ()
{
    int len = sizeof(str[0]);
    printf("the result is %d\n", len);

	STEP(foo(1));
    unsigned long xxx = 810;
    float yyy = 50;

    float zzz = xxx / yyy;
    char str[20];
    sprintf(str, "%2.1f", zzz);
    printf("%s\n",str);
    printf("------\n");

    while (1)
        {

        }
}

